  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/celeryclub/norman-web.git"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "watch:dev": "API_BASE_URL=http://localhost:4000 webpack serve --mode=development",
