    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
